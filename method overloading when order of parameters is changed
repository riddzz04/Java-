class method{
public void info(int id, String name){
System.out.println(name);
System.out.println(id);
}

public void info(String name, int id)
{
System.out.println(id);
System.out.println(name);
}

public static void main(String [] args)
{
method i= new method();
i.info(12,"riddhi");
i.info("raman",123);
}
}

