import java.util.Scanner;
public class arr{
public static void main(String[] args){
Scanner sc= new Scanner(System.in);
int n=sc.nextInt();
 int array[] = new int[n];
int k=sc.nextInt();
int last=-1;
for(int i=0;i<n;i++)
{
array[i]=sc.nextInt();
}

for(int i=0;i<n;i++)
{ 
 if (array[i]==k)
{
last=i;
}

}

System.out.print(last);







}
}
