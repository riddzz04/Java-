import java.util.Scanner;
public class count{
public static void main(String[] args)
{
Scanner sc= new Scanner(System.in);
int decimal= sc.nextInt();
int binary=0;
int power=1;
while(decimal!=0)
{ int parity=decimal%2;
binary= decimal+parity*power;
power = power *10;
decimal/=2;
}
System.out.print(binary);


}
}


