import java.util.Scanner;
public class arr{
public static void main(String[] args){
Scanner sc= new Scanner(System.in);
int n=sc.nextInt();
 int array[] = new int[n];
int x=sc.nextInt();
int count=0;
for(int i=0;i<n;i++)
{
array[i]=sc.nextInt();
}


for(int i=0;i<n;i++)
{ 
 for(int j=i+1;j<n;j++)
{
if (array[i] +array[j]==x)
{
count++;
System.out.println(+array[i] +" " +array[j]);
}

}
}

}
}
