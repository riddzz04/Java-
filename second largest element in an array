import java.util.Scanner;
class pattern{
public static void main(String[] args)
{

Scanner sc= new Scanner(System.in);
int n=sc.nextInt();

int arr[]= new int [n];
int max=arr[0];
int smax= arr[0];

for(int i=0;i<n;i++)
{
arr[i]=sc.nextInt();
}


for(int i=0;i<n;i++)
{
if(arr[i]>max)
{
max= arr[i];
}
}
for(int i=0;i<n;i++)
{

if(arr[i]>smax && arr[i] != max)
{
smax= arr[i];
}
}


System.out.println(smax);
}





}



