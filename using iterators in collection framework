import java.util.*;
public class ll
{
    public static void main(String[] args)
    {
        ArrayList<Integer> l=new ArrayList<>();
        
        //adding elements
        l.add(1);
        l.add(2);
        l.add(3);
        l.add(4);
        System.out.println(l);
        
        Iterator<Integer> it= l.iterator();
        while(it.hasNext())
        {
            System.out.println(it.next());
            
        }
        
        }
        }
        
