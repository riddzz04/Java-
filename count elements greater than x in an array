import java.util.Scanner;
public class arr{
public static void main(String[] args){
Scanner sc= new Scanner(System.in);
int n=sc.nextInt();
 int array[] = new int[n];
int k=sc.nextInt();
int count=0;
for(int i=0;i<n;i++)
{
array[i]=sc.nextInt();
}

for(int i=0;i<n;i++)
{ 
 if (array[i]>k)
{
count++;
}

}

System.out.print(count);







}
}
